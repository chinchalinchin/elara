<iframe
    width="{{ maps_width }}"
    height="{{ maps_height }}"
    style="border:0;"
    loading="lazy"
    allowfullscreen
    referrerpolicy="no-referrer-when-downgrade"
    src="https://www.google.com/maps/embed/v1/place?key={{ maps_api_key }}&q={{ map_latitude }},{{ map_longitude }}">
</iframe>